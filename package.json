{
  "name": "@big-whale-labs/seal-hub-kit",
  "version": "0.0.23",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BigWhaleLabs/seal-hub-kit"
  },
  "files": [
    "dist",
    "contracts/**/*.sol"
  ],
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "np": {
    "2fa": false,
    "tests": false,
    "cleanup": false,
    "yarn": false
  },
  "scripts": {
    "start": "tsc-watch --skipLibCheck --onSuccess 'node dist/src/index.js'",
    "build": "tsc --skipLibCheck && yarn build-contract",
    "build-contract": "yarn hardhat clean && yarn hardhat compile",
    "release": "np --any-branch",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 --ext ts,tsx,json src",
    "prepublishOnly": "yarn lint && yarn build"
  },
  "devDependencies": {
    "@types/elliptic": "^6.4.14",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitejs/plugin-legacy": "^3.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "hardhat": "^2.12.6",
    "np": "^7.6.3",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0",
    "typechain": "^8.1.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^1.7.1"
  },
  "packageManager": "yarn@3.3.1",
  "dependencies": {
    "@big-whale-labs/constants": "^0.1.50",
    "@big-whale-labs/seal-hub-contract": "^0.0.7",
    "@ethereumjs/util": "^8.0.3",
    "@zk-kit/incremental-merkle-tree": "^1.0.0",
    "bn.js": "^5.2.1",
    "circomlibjs": "^0.1.7",
    "elliptic": "^6.5.4",
    "ethers": "^5.7.2"
  }
}
